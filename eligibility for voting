#include<iostream>
using namespace std;

int main() {

    string name;
    int age;
    cout << "Enter name of the user: \n";
    cin >> name;
    cout << "\n\nEnter age of a user: \n";
    cin>>age;

    if (age >= 18) {
        cout << "Congratulations\t" << name << "!!!\nYou are eligible for voting";
    } else {
        cout << "Uh OHH\t" << name <<"...\nYou are not eligible for voting";
    }

    return 0;
}
