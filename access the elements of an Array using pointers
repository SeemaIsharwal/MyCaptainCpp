#include <iostream>
using namespace std;

int main()
{
   int A[7];
   cout << "Enter 7 elements you want to put in array: \n";

   for(int i = 0; i < 7; ++i)
      cin >> A[i];

   cout << "Elements in your array are: \n";
   for(int i = 0; i < 7; ++i)
      cout << endl << *(A + i);

   return 0;
}
